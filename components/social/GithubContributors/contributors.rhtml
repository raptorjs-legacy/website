<c:template
    xmlns:c="core"
    xmlns:w="widgets"
    name="components/social/GithubContributors/contributors"
    params="contributors">
    

    <div class="contributor" c:for="contributor in contributors">
        <img src="$contributor.avatar_url" class="avatar"/>
        <span class="login">
            <a href="$contributor.html_url" target="_blank">
                $contributor.login
            </a>
        </span>
    </div>

</c:template>