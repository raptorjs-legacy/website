<c:template
    xmlns:c="core"
    xmlns:w="widgets"
    name="components/social/TwitterTweet"
    params="url,text,via,size">
    
    <a href="https://twitter.com/share" 
        class="twitter-share-button" 
        data-lang="en"
        data-text="$text"
        data-via="$via"
        data-url="$url"
        data-size="$size">

        Tweet
    </a>

</c:template>